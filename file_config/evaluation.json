{   
    "name": "AG-Reflow_unpaired_10_memorybank_l1-lpips-fake",
    "dataset":{
        // "dir_A": "SynthRAD2023/Task1/brain/3D/segmentation/prior/testA2B"
    },
    "reconstruction": {
        "conduct_reconstruction": false,
        "pattern": "^(.+?)_(\\d+)_(real_A|real_B|fake_B)" 
    },
    "segmentation":{
        "conduct_segmentation": false,
        "task": "total",
        "ml": true,
        "modality": "ct"
    },
    "metrics":{
        // "pattern": "^[1-2](AB|HN|TH)[A-E]\\d{3}",
        // "pattern": "^[1-2][B|P][A-C]\\d{3}$",
        "pattern": "^\\d+[A-Za-z]+\\d+$", // 用于匹配SynthRAD202X数据集中的文件名格式
        "image_similarity":{
            "calculate_metrics": true,
            "metric_names": ["MAE","FID","SSIM","PSNR"],
            "dynamic_range": [-1024, 2000]
            // "pattern": "^[1-2](HN)[A-E]\\d{3}",
            // "pattern": "^[1-2][B|P][A-C]\\d{3}$"
            // "class_range": [[-1024,-250],[-250,250],[250,2000]]
        },
        "segmentation":{
            "calculate_metrics": true,
            "metric_names": ["DICE"],
            "task": "total",
            "ml": true
        }
    },
    "result":{
        "result_dir": "./file_result",
        "test_epoch": "best"
    }
}